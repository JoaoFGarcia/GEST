unit uModel_CNAE;

interface

uses uAbstract_Model;

type
  [TTable('CNAE')]
  TModelQSA = class(TModel)
  private
    FID: Integer;
    FNOME: String;
    FCODIGO_CNAE: Integer;
  published
    [TField('IDentificador', [dbKey])]
    property ID: Integer read FID write FID;
    [TField('Código CNAE', [dbUpdate])]
    property NOME: String read FNOME write FNOME;
    [TField('Descrição', [dbUpdate])]
    property CODIGO: Integer read FCODIGO write FCODIGO;
  end;

implementation

end.
