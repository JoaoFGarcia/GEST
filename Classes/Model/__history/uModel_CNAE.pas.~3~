unit uModel_CNAE;

interface

uses uAbstract_Model;

type
  [TTable('CNAE')]
  TModelQSA = class(TModel)
  private
    FID: Integer;
    FDESCRICAO: String;
    FCODIGO_CNAE: Integer;
  published
    [TField('IDentificador', [dbKey])]
    property ID: Integer read FID write FID;
    [TField('Código CNAE', [dbUpdate])]
    property CODIGO_CNAE: Integer read FCODIGO_CNAE write FCODIGO_CNAE;
    [TField('Descrição', [dbUpdate])]
    property DESCRICAO: String read FDESCRICAO write FDESCRICAO;
  end;

implementation

end.
