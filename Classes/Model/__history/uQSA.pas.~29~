unit uQSA;

interface

uses
  uAbstract_Model;

type
  TQSA = class(TModel)
  private
    FID     : Integer;
    FNOME   : String;
    FCODIGO : Integer;
  published
    [TField('IDentificador', [dbKey, dbWhere])]
    property ID : Integer read FID write FID;
    [TField('IDentificador', [dbWhere, dbUpdate])]
    property ID : Integer read FID write FID;
  end;

implementation

end.
