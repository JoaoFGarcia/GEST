unit uModel_QSA;

interface

uses uAbstract_Model;

type
  TModel_QSA = class(TModel)
  private
    FID: Integer;
    FNOME: String;
    FCODIGO: Integer;
  published
    [TField('IDentificador', [dbKey, dbWhere])]
    property ID: Integer read FID write FID;
    [TField('Nome', [dbWhere, dbUpdate])]
    property NOME: String read FNOME write FNOME;
    [TField('Código', [dbWhere, dbUpdate])]
    property CODIGO: Integer read FCODIGO write FCODIGO;
  end;

implementation

end.
