unit uFornecedor;

interface

uses
  uAbstract_Model,
  System.Generics.Collections,
  System.SysUtils,
  uQSA;

type
  TFornecedor = class(TModel)
  private
    FID                      : Integer;
    FATIVIDADE_PRINCIPAL     : Integer;
    FDESCATIVIDADE_PRINCIPAL : String;
    FDATA_SITUACAO           : TDateTime;
    FTIPO                    : String;
    FNOME                    : String;
    FUF                      : String;
    FTELEFONE                : String;
    FEMAIL                   : String;
    FQSAS                    : TObjectList<TQSA>;
    FSITUACAO                : String;
    FBAIRRO                  : String;
    FLOGRADOURO              : String;
    FNUMERO                  : String;
    FCEP                     : Integer;
    FMUNICIPIO               : String;
    FPORTE                   : String;
    FABERTURA                : TDateTime;
    FNATUREZA_JURIDICA       : String;
    FCNPJ                    : Integer;
    FNOME_FANTASIA           : String;
    FCOMPLEMENTO             : String;
    FENTE_FEDERATIVO         : String;
    FSITUACAO_ESPECIAL       : String;
    FMOTIVO_SITUACAOESP      : String;
    FDATA_SITUACAOESP        : String;
    FCAPITAL_SOCIAL          : Double;
  published
    [TField('Código', [dbKey, dbWhere])]
    property ID                     : Integer read FID write FID;
    [TField('ID Atividade Principal', [dbWhere, dbUpdate, dbForeignWhere])]
    property ATIVIDADE_PRINCIPAL    : Integer read FATIVIDADE_PRINCIPAL write FATIVIDADE_PRINCIPAL;
    [TField('Descrição Atividade Principal', [dbForeign])]
    property DESCATIVIDADE_PRINCIPAL: String read FDESCATIVIDADE_PRINCIPAL write FDESCATIVIDADE_PRINCIPAL;
    [TField('Data Situação', [dbWhere, dbUpdate])]
    property DATA_SITUACAO          : TDateTime read FDATA_SITUACAO write FDATA_SITUACAO;
    [TField('Tipo', [dbWhere, dbUpdate])]
    property TIPO                   : String read FTIPO write FTIPO;
    [TField('Nome', [dbWhere, dbUpdate])]
    property NOME                   : String read FNOME write FNOME;
    [TField('Nome fantasia', [dbWhere, dbUpdate])]
    property NOME_FANTASIA                   : String read FNOME_FANTASIA write FNOME_FANTASIA;
    [TField('Sigla Estado', [dbWhere, dbUpdate])]
    property UF                     : String read FUF write FUF;
    [TField('Telefone', [dbWhere, dbUpdate])]
    property TELEFONE               : String read FTELEFONE write FTELEFONE;
    [TField('E-mail', [dbWhere, dbUpdate])]
    property EMAIL                  : String read FEMAIL write FEMAIL;
    [TField('Qualificação societária', [dbWhere, dbUpdate, dbForeignWhere])]
    property QSAS                   : TObjectList<TQSA> read FQSAS write FQSAS;
    [TField('Situação', [dbWhere, dbUpdate])]
    property SITUACAO               : String read FSITUACAO write FSITUACAO;
    [TField('Bairro', [dbWhere, dbUpdate])]
    property BAIRRO                 : String read FBAIRRO write FBAIRRO;
    [TField('Logradouro', [dbWhere, dbUpdate])]
    property LOGRADOURO             : String read FLOGRADOURO write FLOGRADOURO;
    [TField('Complemento', [dbWhere, dbUpdate])]
    property COMPLEMENTO             : String read FCOMPLEMENTO write FCOMPLEMENTO;
    [TField('Número', [dbWhere, dbUpdate])]
    property NUMERO                 : String read FNUMERO write FNUMERO;
    [TField('CEP', [dbWhere, dbUpdate])]
    property CEP                    : Integer read FCEP write FCEP;
    [TField('Município', [dbWhere, dbUpdate])]
    property MUNICIPIO              : String read FMUNICIPIO write FMUNICIPIO;
    [TField('Porte', [dbWhere, dbUpdate])]
    property PORTE                  : String read FPORTE write FPORTE;
    [TField('Abertura', [dbWhere, dbUpdate])]
    property ABERTURA               : TDateTime read FABERTURA write FABERTURA;
    [TField('Natureza Jurídica', [dbWhere, dbUpdate])]
    property NATUREZA_JURIDICA      : String read FNATUREZA_JURIDICA write FNATUREZA_JURIDICA;
    [TField('CNPJ', [dbWhere, dbUpdate])]
    property CNPJ                   : Integer read FCNPJ write FCNPJ;
    [TField('Ente Federativo', [dbWhere, dbUpdate])]
    property ENTE_FEDERATIVO        : String read FENTE_FEDERATIVO write FENTE_FEDERATIVO;
    [TField('Situação Especial', [dbWhere, dbUpdate])]
    property SITUACAO_ESPECIAL        : String read FSITUACAO_ESPECIAL write FSITUACAO_ESPECIAL;
    [TField('Motivo Situação Especial', [dbWhere, dbUpdate])]
    property MOTIVO_SITUACAOESP        : String read FMOTIVO_SITUACAOESP write FMOTIVO_SITUACAOESP;
    [TField('Data Situação Especial', [dbWhere, dbUpdate])]
    property DATA_SITUACAOESP        : String read FDATA_SITUACAOESP write FDATA_SITUACAOESP;
    [TField('Capital Social', [dbWhere, dbUpdate])]
    property CAPITAL_SOCIAL        : Double read FCAPITAL_SOCIAL write FCAPITAL_SOCIAL;

    public
      constructor Create();
      destructor Destroy();
  end;

implementation

{ TFornecedor }

constructor TFornecedor.Create;
begin
  FQSAS := TObjectList<TQSA>.Create;
end;

destructor TFornecedor.Destroy();
begin
  if Assigned(FQSAS) then
    FreeAndNil(FQSAS);
end;

end.
